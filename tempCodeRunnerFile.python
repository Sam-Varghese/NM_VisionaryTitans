class abc:
    def __init__(self):
        self.var = 5
    def change_var(self):
        print("Attempted to change value of var to 10")
        self.var = 10

if __name__ == '__main__':
    import multiprocessing
    manager = multiprocessing.Manager()

    a = manager.dict()
    a[1] = [abc(), abc()]

    for i in range(2):
        a[1][i].change_var()

    for i in range(2):
        print(a[1][i].var)